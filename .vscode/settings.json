{
  "workbench.colorCustomizations": {
    "activityBar.background": "#030b32",
    "titleBar.activeBackground": "#050946",
    "titleBar.activeForeground": "#FDFBE1"
  },
  // See https://code.visualstudio.com/docs/getstarted/settings#_settingsjson
  // for the documentation about the `settings.json` format.
  "editor.codeActionsOnSave": {
    "source.fixAll.markdownlint": "explicit"
  },
  "files.trimTrailingWhitespace": true,
  "extensions.ignoreRecommendations": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "cSpell.enabled": true,
  "cSpell.words": []
}
